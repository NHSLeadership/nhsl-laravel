{
    "name": "nhsleadership/nhsl-laravel",
    "description": "NHSL theme for Laravel based upon NHSUK frontend framework",
    "homepage": "https://github.com/NHSLeadership/nhsl-laravel",
    "version": "1.2.0",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "Nhsl\\NhslLaravel\\": "src/",
            "Nhsl\\NhslLaravel\\Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nhsl\\NhslLaravel\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Marcin Calka",
            "email": "marcin.calka@nhsx.uk"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Nhsl\\NhslLaravel\\NhslLaravelServiceProvider"
            ],
            "branch-alias": {
                "dev-main": "1.x-dev",
                "dev-master": "1.x-dev"
            }
        }
    },
    "minimum-stability": "dev",
    "require": {
        "composer-plugin-api": ">=2.2.0",
        "orchestra/testbench": ">=5.0",
        "laravel/framework": ">=7.6",
        "phpunit/phpunit": ">=9.4",
        "tinymce/tinymce": ">=6.2"
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "source": "https://github.com/NHSLeadership/nhsl-laravel",
        "issues": "https://github.com/NHSLeadership/nhsl-laravel/issues"
    }
}
